{
  "name": "dext-darwin-applications-plugin",
  "version": "0.1.0",
  "description": "Dext plugin to search for applications (Darwin only).",
  "keywords": [
    "dext",
    "dext-plugin",
    "applications",
    "darwin",
    "mac",
    "osx",
    "search",
    "finder"
  ],
  "main": "index.js",
  "author": "Vu Tran <vu@vu-tran.com>",
  "license": "MIT",
