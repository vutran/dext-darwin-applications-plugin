    "coveralls": "^2.11.14",
    "electron-rebuild": "^1.5.5",
    "eslint": "^3.7.1",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^2.0.0",
    "jest": "^16.0.1"
